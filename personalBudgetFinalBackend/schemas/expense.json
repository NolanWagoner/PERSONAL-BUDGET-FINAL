{
    "$id": "expense.json",
    "type": "object",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
      "title": {
        "title": "title",
        "type": "string",
        "description": "Title for expense that will be sent to database",
        "maxLength": 120,
        "minLength": 1
      },
      "expense": {
        "title": "expense",
        "description": "Expense value that will be sent to database",
        "type": "number",
        "minimum": 0,
        "maximum": 99999999999999999999
      }
    },
    "required": [
      "title",
      "expense"
    ]
  }